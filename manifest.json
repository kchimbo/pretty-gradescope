{
    "name": "Pretty Gradescope",
    "short_name": "Pretty Gradescope",
    "version": "1.1",
    "manifest_version": 2,
    "description": "Format Gradescope results",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [{
        "js": ["content.js"],
        "matches": ["https://www.gradescope.com/courses/*"]
    }],
    "permissions": [
        "https://www.gradescope.com/courses/*"
    ],
    "author": "Osamu Fujimoto"
}